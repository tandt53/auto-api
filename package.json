{
  "name": "auto-api",
  "version": "1.0.0",
  "description": "this is cli for running API tests automatically",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node ./dist/index.js",
    "debug": "ts-node ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-client": "openapi --input ./openapi.json --output src/reference --client axios"
  },
